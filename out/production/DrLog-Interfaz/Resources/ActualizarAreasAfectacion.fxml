<?import javafx.scene.text.Font?>

<GridPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ActulizarBaseDatosMenu">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Prevencion Sintoma 1" GridPane.halignment="CENTER">
               <font>
                  <Font name="AppleGothic Regular" size="20.0" />
               </font>
            </Label>
            <Label text="Prevencion Sintoma 2" GridPane.halignment="CENTER" GridPane.rowIndex="1">
               <font>
                  <Font name="AppleGothic Regular" size="20.0" />
               </font>
            </Label>
            <Label text="Prevencion Sintoma 3" GridPane.halignment="CENTER" GridPane.rowIndex="2">
               <font>
                  <Font name="AppleGothic Regular" size="20.0" />
               </font>
            </Label>
            <Label text="Tratamiento Previo" GridPane.halignment="CENTER" GridPane.rowIndex="3">
               <font>
                  <Font name="AppleGothic Regular" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="prevencionSintoma1" GridPane.columnIndex="1">
               <GridPane.margin>
                  <Insets right="5.0" />
               </GridPane.margin></TextField>
            <TextField fx:id="prevencionSintoma2" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="5.0" />
               </GridPane.margin></TextField>
            <TextField fx:id="prevencionSintoma3" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets right="5.0" />
               </GridPane.margin></TextField>
            <TextField fx:id="tratamientoPrevio" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets right="5.0" />
               </GridPane.margin></TextField>
         </children>
      </GridPane>
      <Button fx:id="ActualizarBoton" mnemonicParsing="false" onAction="#actualizarBaseDatos" text="Actualizar" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" right="5.0" />
         </GridPane.margin>
      </Button>
   </children>
</GridPane>
